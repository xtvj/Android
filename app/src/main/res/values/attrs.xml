<resources>
    <declare-styleable name="CheckCircleView">
        <!--背景色-->
        <attr name="check_background_color" format="color" />
        <!--按压时颜色-->
        <attr name="check_foreground_color" format="color" />
        <!--最外圆环的宽度-->
        <attr name="check_circle_first_width" format="dimension" />
        <!--第二个圆环的宽度-->
        <attr name="check_circle_second_width" format="dimension" />
        <!--第四个圆环的宽度-->
        <attr name="check_circle_fourth_width" format="dimension" />
        <!--第二个圆半径0-1-->
        <attr name="check_radius_second" format="float" />
        <!--第三个圆半径0-1-->
        <attr name="check_radius_third" format="float" />
        <!--第四个圆半径0-1-->
        <attr name="check_radius_fourth" format="float" />
        <!--最外圆渐进开始弧度0-360-->
        <attr name="check_radian_start" format="float" />
        <!--最外圆渐进结束弧度0-360-->
        <attr name="check_radian_end" format="float" />
        <attr name="check_stroke_cap" format="enum">
            <!--默认没有-->
            <enum name="butt" value="0" />
            <!--圆角-->
            <enum name="round" value="1" />
            <!--直角-->
            <enum name="square" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FrequencyView">
        <attr name="frequency_progress" format="float" />
        <attr name="frequency_progress_color" format="color" />
        <attr name="frequency_start_color" format="color" />
        <attr name="frequency_middle_color" format="color" />
        <attr name="frequency_middle2_color" format="color" />
        <attr name="frequency_end_color" format="color" />
        <attr name="frequency_ring_color" format="color" />
        <attr name="frequency_arrow_color" format="color" />
    </declare-styleable>

    <declare-styleable name="DampingReboundFrameLayout">
        <attr name="orientation">
            <enum name="portrait" value="1" />
            <enum name="landscape" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SlidingButton">

        <attr name="sliding_button_width" format="dimension|reference" />
        <attr name="sliding_button_height" format="dimension|reference" />
        <attr name="sliding_button_icon" format="reference" />
        <attr name="sliding_button_icon_tint" format="color|reference" />
        <attr name="sliding_icon_scaleType" format="enum">
            <enum name="centerInside" value="7" />
            <enum name="centerCrop" value="6" />
            <enum name="center" value="5" />
            <enum name="fitCenter" value="3" />
            <enum name="fitXY" value="1" />
        </attr>
        <attr name="sliding_button_background" format="color|reference" />

        <attr name="sliding_trackBackground" format="color|reference" />
        <attr name="sliding_trackBackgroundTint" format="color|reference" />

    </declare-styleable>


</resources>